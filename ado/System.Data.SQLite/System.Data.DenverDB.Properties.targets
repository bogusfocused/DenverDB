<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <UseInteropDll>false</UseInteropDll>
    <UseSqliteStandard>True</UseSqliteStandard>
    <InteropLog>false</InteropLog>
    <BinaryOutputPath>..\..\bin\$(ConfigurationYear)\$(Configuration)\$(Platform)\</BinaryOutputPath>
    <InteropExtensionFunctions>false</InteropExtensionFunctions>
    <InteropTestExtension>false</InteropTestExtension>
    <InteropVirtualTable>false</InteropVirtualTable>
  </PropertyGroup>

</Project>
